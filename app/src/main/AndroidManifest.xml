        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="masterlistas.manunez.org.masterlistas.ListasActivity" />
        <activity android:name="masterlistas.manunez.org.masterlistas.InicioSesionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
